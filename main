#!/usr/bin/env python
import os
def commits(x: int):
    for i in range(x):
        os.system(f"echo {i} > data.txt")
        os.system(f"git add data.txt")
        os.system(f"git commit -m='{i}' --date='{i} day ago'")
        os.system("git push")
x=int(input("Введите количество коммитов:"))
commits(x)
